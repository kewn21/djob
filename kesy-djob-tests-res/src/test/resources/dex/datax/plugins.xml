<?xml version="1.0" encoding="UTF-8"?>
<plugins>
	<plugin>
		<!-- the version of this plugin -->
		<version>1</version>
		<!-- the name of this plugin, it must be unique in this file -->
		<name>mysqlreader</name>
		<!-- reader | writer | spliter -->
		<type>reader</type>
		<!-- mysql | oracle | hdfs | feitian -->
		<target>mysql</target>
		<!-- pulgin's class name, must include package path -->
		<class>org.kesy.djob.dex.datax.plugins.reader.mysqlreader.MySqlReader</class>
		<!-- max concurrency the plugin can support -->
		<maxthreadnum>10</maxthreadnum>
	</plugin>
	
	<plugin>
		<version>1</version>
		<name>streamreader</name>
		<type>reader</type>
		<target>stream</target>
		<class>org.kesy.djob.dex.datax.plugins.reader.streamreader.StreamReader</class>
		<maxthreadnum>40</maxthreadnum>
	</plugin>
    
	<plugin>
		<version>1</version>
		<name>httpreader</name>
		<type>reader</type>
		<target>http</target>
		<class>org.kesy.djob.dex.datax.plugins.reader.httpreader.HttpReader</class>
		<maxthreadnum>40</maxthreadnum>
	</plugin>
	
	<plugin>
		<version>1</version>
		<name>mysqlwriter</name>
		<type>writer</type>
		<target>mysql</target>
		<class>org.kesy.djob.dex.datax.plugins.writer.mysqlwriter.MySqlWriter</class>
		<maxthreadnum>10</maxthreadnum>
	</plugin>
	
	<plugin>
		<version>1</version>
		<name>streamwriter</name>
		<type>writer</type>
		<target>stream</target>
		<class>org.kesy.djob.dex.datax.plugins.writer.streamwriter.StreamWriter</class>
		<maxthreadnum>10</maxthreadnum>
	</plugin>
	
	<plugin>
		<version>1</version>
		<name>ftpwriter</name>
		<type>writer</type>
		<target>stream</target>
		<class>org.kesy.djob.datax.plugins.writer.ftpwriter.FtpWriter</class>
		<maxthreadnum>10</maxthreadnum>
	</plugin>
</plugins>
